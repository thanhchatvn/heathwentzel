<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_medical_aid_claims" model="ir.sequence">
            <field name="name">Medical Aid Claims</field>
            <field name="code">claim_sequence</field>
            <field name="prefix">CLAIM-</field>
            <field name="padding">6</field>
        </record>

        <record id="mediswitch_payment_acquirers" model="payment.acquirer">
            <field name="name">Mediswitch Payment Gateway</field>
            <field name="provider">transfer</field>
            <field name="company_id" model="res.company" eval="obj().search([], limit=1).id"></field>
        </record>
    </data>
</odoo>